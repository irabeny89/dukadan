{
  "name": "dukadan",
  "version": "1.0.50",
  "scripts": {
    "test": "bun test",
    "html-scan": "xss-scan",
    "dev": "bun run --watch src/index.ts",
    "build": "bun bun-build.js",
    "start": "bun dist/index.js",
    "lint": "bunx biome check --write ./src",
    "unsafe-lint": "bunx biome check --fix --unsafe",
