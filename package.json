{
  "name": "dukadan",
  "version": "1.0.50",
  "scripts": {
    "test": "bun test",
    "dev": "bun run --watch src/index.ts",
    "build": "bun build --compile --minify-whitespace --minify-syntax --target bun --outfile server --outdir ./dist ./src/index.ts",
    "lint": "bunx biome check --write src",
    "unsafe-lint": "bunx biome check --fix --unsafe",
    "db:migrate": "migralite --database ./db/dukadan.sqlite",
    "migration:gen": "migralite -g"
  },
  "dependencies": {
    "@elysiajs/cors": "^1.1.1",
    "@elysiajs/swagger": "^1.1.1",
    "elysia": "latest"
  },
  "devDependencies": {
    "@biomejs/biome": "1.9.2",
    "bun-types": "latest",
    "migralite": "^1.0.1"
  },
  "module": "src/index.js"
}
