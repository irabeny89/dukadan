  },
  "dependencies": {
    "@elysiajs/cors": "^1.1.1",
    "@elysiajs/html": "^1.1.1",
    "@elysiajs/static": "^1.1.1",
    "@elysiajs/swagger": "^1.1.1",
    "@kitajs/ts-html-plugin": "^4.1.0",
    "@types/jsonwebtoken": "^9.0.7",
    "elysia": "latest",
    "jsonwebtoken": "^9.0.2"
  },
  "devDependencies": {
    "@biomejs/biome": "1.9.2",
    "bun-types": "latest",
    "migralite": "^1.0.1"
  },
  "module": "src/index.js"
}
